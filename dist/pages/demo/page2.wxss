.tab-bar-wrap {
  display: flex;
  width: 100%;
  height: 110rpx;
  padding-top: 10rpx;
  font-size: 30rpx;
  line-height: 110rpx;
  background-color: #fff;
  flex-flow: row; }
  .tab-bar-wrap.hasposition {
    position: fixed;
    z-index: 233; }
  .tab-bar-wrap.bottom {
    bottom: 0; }
